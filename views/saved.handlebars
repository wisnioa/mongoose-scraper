<div id="articles">
    {{#each savedArticles}}
    <div class="row">
        <div class="col s12">
            <div class="hoverable card panel">

                <div class="articleCard card-content">

                    {{this.title}}
                    <a id="link" target="_blank" href={{this.link}}></a>
                    <p id="note">{{this.note}}</p>
                    <br>
                    <br>

                    <div class="bttnRow">
                        <a class="saveArticleBtn orange darken-4 btn">
                            <i class="material-icons left">save</i>Save Article</a>
                        <a class="writeNoteBtn orange darken-4 btn">
                            <i class="material-icons left">edit</i>Write Note</a>
                    </div>
                </div>
                {{!--
                <button id="makeNote">Write a Note</button> --}} {{!--
                <button id="saveBtn">Save Article</button> --}}
            </div>
        </div>
    </div>

    {{/each}}
</div>